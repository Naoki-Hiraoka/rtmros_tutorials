<robot name="simple_box05">
  <link name="nil_link">
    <inertial>
      <origin xyz="0 0 0" /> 
      <mass value="2" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 0.02" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 0.02" />
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>1.0</mu>
            <mu2>1.0</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
  </link>
  <gazebo reference="nil_link">
    <material>Gazebo/Blue</material>
  </gazebo>
  <gazebo reference="nil_link">
    <mu1>2.0</mu1>
    <mu2>2.0</mu2>
    <kp>1400000.0</kp>
    <kd>280.0</kd>
    <fdir1>1 0 0</fdir1>
    <maxVel>10.0</maxVel>
    <minDepth>0.00</minDepth>
  </gazebo>
  <gazebo>
    <static>true</static>
  </gazebo>
